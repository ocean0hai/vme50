{
    "easycom": {
        "autoscan": true,
        "custom": {
            "^o-(.*)": "@/components/order/$1/$1.vue",
            "^i-(.*)": "@/components/index/$1/$1.vue",
            "^a-(.*)": "@/components/address/$1/$1.vue",
            "^ad-(.*)": "@/components/admin/$1/$1.vue",
            "^r-(.*)": "@/components/records/$1/$1.vue"
        }
    },
    "pages": [
        {
            "path": "pages/index/index",
            "name": "index",
            "style": {
                "navigationBarTitleText": "vme50系统"
            }
        },
        {
            "path": "pages/address/index",
            "name": "address",
            "style": {
                "navigationBarTitleText": "地址管理"
            }
        },
        {
            "path": "pages/order/index",
            "name": "order",
            "style": {
                "navigationBarTitleText": "订单"
            }
        },
        {
            "path": "pages/records/index",
            "name": "records",
            "style": {
                "navigationBarTitleText": "订单记录"
            }
        },
        {
            "path": "pages/login/index",
            "name": "login",
            "style": {
                "navigationBarTitleText": ""
            }
        },
        {
            "path": "pages/admin/index",
            "name": "admin",
            "style": {
                "navigationBarTitleText": "管理"
            }
        }
    ],
    "globalStyle": {
        "navigationBarTextStyle": "black",
        "navigationBarTitleText": "uni-app",
        "navigationBarBackgroundColor": "#F8F8F8",
        "backgroundColor": "#F8F8F8",
        "usingComponents": {
            "van-sidebar": "/wxcomponents/dist/sidebar/index",
